{
  "name": "Google Translate Plus",
  "short_name": "GTPlus",
  "description": "__MSG_extDescription__",
  "version": "0.0.4.0",
  "default_locale": "en",
  "icons": { "16": "image/icon.16.png",
             "48": "image/icon.48.png",
             "128": "image/icon.128.png" },
  "content_scripts":  [{
      "matches": ["https://translate.google.com/*","https://translate.google.com.tw/*"],
      "js": ["lib/jquery-3.1.0.min.js",
             "content.js"],
	  "css": [ "css/style.css"]
  }],
  "browser_action": {
    "default_title": "Google Translate Plus",
	"default_icon" : "image/icon.128.png"
  },
  "permissions": [
    "https://translate.googleapis.com/*"
  ],
  "manifest_version": 2,
  "key": ""
}
